<header class="mb-5  bg-img">
  <div class="container">
    <div class="row justify-content-between">
      <div class="col">
        <h1 class=""><a href="/">Iceland Dashboard</a></h1>
      </div>
      <div class="col text-end">
        <% if(user && (user.status === 'admin')) { %>
          <a class="btn btn-primary" href="/dashboard/user-panel">User Panel</a>
        <% } %>
        <% if (!user) {%>
        <a class="btn btn-primary" href="/dashboard/log-in">LOG IN</a>
        <a class="btn btn-primary" href="/dashboard/signup">Sign Up</a>
        <%} else {%> 
          <a class="btn btn-primary" href="/dashboard/log-out">LOG OUT</a>
        <% } %>
      </div>
    </div>
  </div>
</header>
